{
  "description": "Intercepting browser requests",
  "manifest_version": 2,
  "name": "Intercept Plugin",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "icons": {
    "48": "icons/intercept-48.png",
    "96": "icons/intercept-96.png"
  },

  "browser_action": {
    "default_icon": {
      "16": "icons/intercept-16.png",
      "32": "icons/intercept-32.png"
    },
    "default_title": "Intercept Plugin"
  },
  "devtools_page": "devtools_page.html"
}
